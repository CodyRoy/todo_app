<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @list.title %>
</p>

<p>
  <strong>Desc:</strong>
  <%= @list.desc %>
</p>

<p>
  <strong>Tasks:</strong>
  <%@list.tasks.each do |task| %>
    <p>
    <%= task.content %> |
    <%= link_to 'Details', list_task_path(task,@list)%> | 
    <%= link_to 'Edit', edit_list_task_path(list_id: @list.id, id: task.id)%>
    </p>
  <% end %>
</p>

<%= link_to 'Edit', edit_list_path(@list) %> |
